  "description": "A collection of typescript functions and structures that I usually end up needing when developing.",
  "type": "commonjs",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "scripts": {
    "prepublishOnly": "npm run lint && npm run test && npm run build",
