  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.2"
  }
